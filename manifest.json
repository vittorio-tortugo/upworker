{
  "manifest_version": 2,

  "name": "Upworker",
  "version" : "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.upwork.com/*"],
      "js": ["main.js"]
    }
  ]
}
